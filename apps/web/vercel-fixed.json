{
  "version": 2,
  "name": "29cards-fixed",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://29cards-api.railway.app",
    "NEXT_PUBLIC_GAME_SERVICE_URL": "https://29cards-api.railway.app"
  }
}