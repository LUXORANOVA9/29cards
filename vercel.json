{
  "version": 2,
  "name": "29cards",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://29cards-api.railway.app",
    "NEXT_PUBLIC_WS_URL": "wss://29cards-api.railway.app"
  },
  "functions": {
    "apps/web/pages/**/*.js": {
      "maxDuration": 30
    }
  }
}